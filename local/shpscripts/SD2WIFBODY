<script language="JavaScript">                                                  
// Note: 1.1c Added to fix auto add bug                                         
// Code: gets keypress value for supported browsers                             
function fnGetKeyValue(e)                                                       
{                                                                               
    e = (e) ? e : ((event) ? event : null);                                     
    return (document.layers && Event) ? e.which : e.keyCode;                    
}                                                                               
</script>                                                                       
<noscript>JavaScript is Required!</noscript>                                    
<body class="WhatIfBody" onLoad="SetDefaultValues()">                           
<$ILINCLUDE SCRIPT=SD2GENFRM>                                                   
 name="frmWhatIfBody">                                                          
