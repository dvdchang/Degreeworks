<$ILCGIMASK ACTION="SEPUSAGE">                                                  
<script>                                                                        
function ToggleWhatIf()                                                         
{                                                                               
    myWhatIfSetting = document.frmWhatIfBody.WHATIFSETTING;                     
    if (myWhatIfSetting.value == "OFF")                                         
    {                                                                           
        myWhatIfSetting.value = "ON";                                           
        document.frmWhatIfBody.ToggleWhatIfButton.value="Hide What If Options"; 
        parent.document.all("LeftSide").rows="15%,85%";                         
    }                                                                           
    else                                                                        
    {                                                                           
        myWhatIfSetting.value = "OFF";                                          
        document.frmWhatIfBody.ToggleWhatIfButton.value="Show What If Options"; 
        parent.document.all("LeftSide").rows="*,60";                            
    }                                                                           
}                                                                               
                                                                                
function ToggleWhatIfCheckbox()                                                 
{                                                                               
    if (document.frmWhatIfBody.BLOCKS.length > 0)                               
    {                                                                           
        document.frmWhatIfBody.UseWhatIf.checked = true;                        
    }                                                                           
    else                                                                        
    {                                                                           
        document.frmWhatIfBody.UseWhatIf.checked = false;                       
    }                                                                           
}                                                                               
</script>                                                                       
<table cellspacing="0" cellpadding="0" border="0" width="100%"                  
>                                                                               
    <tr>                                                                        
        <td align="left">                                                       
            <input type="hidden" name="WHATIFSETTING" value="OFF">              
            <input type="button" name="ToggleWhatIfButton"                      
             value="Show What If Options" class="ExceptionButton"               
             onclick="ToggleWhatIf();">                                         
        </td>                                                                   
        <td align="right">                                                      
            <input type="checkbox" name="UseWhatIf" id="UseWhatIf" 
	           class="myCheckbox">            
            <label for="UseWhatIf" class="WhatIfInput">                         
            Use What If Scenario                                                
            </label>                                                            
        </td>                                                                   
    </tr>                                                                       
</table>                                                                        
<br />                                                                          
</$ILCGIMASK>                                                                   
