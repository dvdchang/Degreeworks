// 1.1g End                                                                     
    function SetDefaultValues()                                                 
    {                                                                           
        if (bShowLookAheadSection)              // 1.04                         
            LoadClasses('<$ILCGI-CLASSES>', document.frmWhatIfBody.CLASSES);    
        var bSetSchool = false;                                                 
        var bSetDegree = false;                                                 
        // 1.1i Start                                                           
        CFG020CatYear = Trim("<$ILUCX SRC=UCXCFG020 KEY=WHATIF BYTES=35:12>");        
        if (CFG020CatYear == "")                                                   
        {//Set to student's catalog year                                        
           SetDefault (document.forms[0].CATYEAR, "<$ILSTU*SDCATYEAR>");        
        }                                                                       
        else                                                                    
        {//Set to catalog year defined in UCXCFG020 WHATIF                         
           SetDefault (document.forms[0].CATYEAR, CFG020CatYear);                  
        }                                                                       
        // 1.1i End                                                             
        // If we are to Show the school picklist                                
<$ILUCXMASK SRC=UCXCFG020 KEY=WHATIF BYTES=32:1 value=Y>                           
        bSetSchool = true;                                                      
</$ILUCXMASK>                                                                   
<$ILUCXMASK SRC=UCXCFG020 KEY=WHATIF BYTES=33:1 value=Y>                           
        bSetDegree = true;                                                      
</$ILUCXMASK>                                                                   
<$ILUCXMASK SRC=UCXCFG020 KEY=WHATIF BYTES=2:1 value=D>                            
        // MajorDrivesDegree=D                                                  
        bSetDegree = false;                                                     
        bSetSchool = false;                                                 
</$ILUCXMASK>                                                                   
        if (bSetSchool)                                                         
          SetDefault(document.forms[0].SCHOOL,  "<$ILCGI-STUSCH>");             
        if (bSetDegree)                                                         
          SetDefault (document.forms[0].DEGREE,  "<$ILCGI-STUDEG>");            
<$ILUCXMASK SRC=UCXCFG020 KEY=WHATIF BYTES=1:1 value=M>                            
        // DegreeDrivesMajor=M                                                  
<$ILUCXMASK Not SRC=UCXCFG020 KEY=WHATIF BYTES=4:1 value=N>                            
        FilterPicklists('MAJOR'); // 1.1h                                       
</$ILUCXMASK>                                                                   
</$ILUCXMASK>                                                                   
    }                                                                           
    function SetDefault(oSelect, sDefaultValue)                                 
    {                                                                           
        var iIndex = 0;                                                         
        if (sDefaultValue.length == 0)                                          
        {                                                                       
            return;                                                             
        }                                                                       
        if (typeof(oSelect) == "undefined")                                     
        {                                                                       
            return;                                                             
        }                                                                       
        while (iIndex < oSelect.length)                                         
        {                                                                       
            if (Trim(oSelect.options[iIndex].value) == sDefaultValue)           
            {                                                                   
                // Select this report as the default                            
                oSelect.selectedIndex = iIndex;                                 
            }                                                                   
            iIndex++;                                                           
        } // while                                                              
    } // setdefault                                                             

    ///////////////////////////////////////////////////                         
    // PROCESSBLOCKS                                                            
    //                                                                          
    ///////////////////////////////////////////////////                         
    function ProcessBlocks()                                                    
    {                                                                           
        var oBlocks = document.frmWhatIfBody.BLOCKS;                            
        var oCatYear = document.frmWhatIfBody.CATYEAR;
        var sBlockList = "";                                                    
        var iBlockIndex = 0;                                                         
		    var iProgramCount = 1;
        var iMajorCount = 0;
        var sBlockValue = "";
        var sCatalogYear = 
        Trim(oCatYear.options[oCatYear.selectedIndex].value);
                                                                                
        while (iBlockIndex < oBlocks.length)                                         
        {                                                                       
            sType = oBlocks.options[iBlockIndex].text.substring (0, 7);              
            iColonLocation = sType.indexOf(":");// firefox                      
            if (iColonLocation > -1)                                            
            {                                                                   
                sType = sType.replace(":", " "); // firefox                     
            }
            sBlockValue = Trim(oBlocks.options[iBlockIndex].value);

            if (Trim(sType) == "PROGRAM")
            {                         
              sBlockList = sBlockList + 'GOALCODE=PROGRAM&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr
            }
            else if (Trim(sType) == "MAJOR")
            {                         
              sBlockList = sBlockList + 'GOALCODE=MAJOR&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr

              <$ILUCXMASK SRC=UCXCFG020 KEY=WHATIF BYTES=2:1 value=D>                            
                // WHATIF Major-Drives-Degree=D; use Degree/School/College associated with major
                if (iMajorCount == 0)
                {
                  document.frmWhatIfBody.MAJORDRIVESDEGREE_DEGREE.value = 
                    Get_MJMN_Attributed_Code(Trim(sBlockValue), "DEGREE");
                  document.frmWhatIfBody.MAJORDRIVESDEGREE_SCHOOL.value = 
                    Get_MJMN_Attributed_Code(Trim(sBlockValue), "SCHOOL");
                  iMajorCount++;
                }
                sCollege = Get_MJMN_Attributed_Code(Trim(sBlockValue), "COLLEGE");
                sBlockList = sBlockList + 'GOALCODE=COLLEGE&GOALVALUE="' + sCollege + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; // overall catyr
              </$ILUCXMASK>
              <$ILUCXMASK SRC=UCXCFG020 KEY=WHATIF BYTES=2:1 value=C>                            
                // WHATIF Major-Drives-Degree=C; use College associated with major
                sCollege = Get_MJMN_Attributed_Code(Trim(sBlockValue), "COLLEGE");
                if (sCollege != "")
                  {
                    sBlockList = sBlockList + 'GOALCODE=COLLEGE&GOALVALUE="' + sCollege + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; // overall catyr
                  }
              </$ILUCXMASK>
            }
            else if (Trim(sType) == "MINOR")
            {                         
              sBlockList = sBlockList + 'GOALCODE=MINOR&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr
            }
            else if (Trim(sType) == "CONC")
            {                         
              sBlockList = sBlockList + 'GOALCODE=CONC&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr
            }
            else if (Trim(sType) == "LIBL")
            {                         
              sBlockList = sBlockList + 'GOALCODE=LIBL&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr
            }
            else if (Trim(sType) == "SPEC")
            {                         
              sBlockList = sBlockList + 'GOALCODE=SPEC&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr
            }
            else if (Trim(sType) == "COLLEGE")
            {                         
              sBlockList = sBlockList + 'GOALCODE=COLLEGE&GOALVALUE="' + sBlockValue + '"&' + 'GOALCATYR=' + sCatalogYear + '&'; //+ // overall catyr
            }
            else
            {
              // invalid type -- do nothing 
            }


            iBlockIndex++;                                                           
        } // while                                                              

        //oBlockList.value = "&" + sBlockList;                                  
        sBlockList = "&" + sBlockList;                                          
        //alert ("Blocklist = " + sBlockList);                                  
        return sBlockList;                                                      
    } // processblocks                                                          
                                                                                
    ///////////////////////////////////////////////////                         
    // Get_MJMN_Attributed_Code
	// Gets the degree, college, school from the major array
	// sCodeType can be "DEGREE", "COLLEGE", or "SCHOOL"
	// This is only invoked if the CFG020 WHATIF MajorDrivesDegree = D
    ///////////////////////////////////////////////////                         
    function Get_MJMN_Attributed_Code(sMJMN, sCodeType)                                 
    {                                                                           
        // sAUD027Picklist: Picklist for Majors                                    
        bCodeFound = false;                                                  
        CurrentPosition = 0;                                                    
		// Look through sAUD027Picklist for the Major Code specified           
		MajorPicklistLength = sAUD027Picklist.length;                          
	    sMJMN = Trim(sMJMN);
		while (CurrentPosition < MajorPicklistLength && ! bCodeFound)    
			// while we have not yet found the Major code and we have not   
			// reached the end of the major code picklist                   
		{                                                                   
			if (Trim(sAUD027Picklist[CurrentPosition].major) == sMJMN)   
			{   
				if (sCodeType == "COLLEGE")
				{
					return sAUD027Picklist[CurrentPosition].college;
				}
				else if (sCodeType == "DEGREE")
				{
					return sAUD027Picklist[CurrentPosition].singleDegree;
				}
				else if (sCodeType == "SCHOOL")
				{
					return sAUD027Picklist[CurrentPosition].school;
				}
				else // invalid option
				{
					return "";
				}
				bCodeFound = true;  
													 
			}                                                               
			CurrentPosition++;                                              
		}                                                                   
	    return ""; // in case we get to the end
    }                                                                          
    // 1.1h Start                                                               
    ///////////////////////////////////////////////////                         
    // FilterPicklists                                                          
    ///////////////////////////////////////////////////                         
    function FilterPicklists(FilterType, FilterOn)                              
    {                                                                           
        myMajor = "" // 1.03                                                    
        oForm = document.frmWhatIfBody;                                         
        // Filter MAJOR picklist based on DEGREE                                
        if (FilterType == "MAJOR")                                              
        {                                                                       
           // Clear BLOCKS list since the Degree has changed                    
           oForm.BLOCKS.length = 0;                                             
           // Clear picklist                                                    
           oForm.MAJOR.length = 0;                                              
           myDegree = oForm.DEGREE;                                             
           myDegree = Trim(myDegree.options[myDegree.selectedIndex].value); 
           PicklistLen = 0;                                                     
           oForm.MAJOR.options[PicklistLen] = new Option("Pick a Major", "",0,0)
                                                                                
           PicklistLen++;                                                       
           // Load Picklist using sAUD027Picklist                                  
           for (i=0; i < sAUD027Picklist.length; i++)                              
           {                                                                    
              if (myDegree == sAUD027Picklist[i].degree1  ||           
                  myDegree == sAUD027Picklist[i].degree2  ||           
                  myDegree == sAUD027Picklist[i].degree3  ||           
                  myDegree == sAUD027Picklist[i].degree4  ||           
                  myDegree == sAUD027Picklist[i].degree5  ||           
                  myDegree == sAUD027Picklist[i].degree6  ||           
                  myDegree == sAUD027Picklist[i].degree7  ||           
                  myDegree == sAUD027Picklist[i].degree8  ||           
                  myDegree == sAUD027Picklist[i].degree9  ||              
                  myDegree == sAUD027Picklist[i].degree10 ||
                  myDegree == sAUD027Picklist[i].degree11  ||           
                  myDegree == sAUD027Picklist[i].degree12  ||           
                  myDegree == sAUD027Picklist[i].degree13  ||           
                  myDegree == sAUD027Picklist[i].degree14  ||           
                  myDegree == sAUD027Picklist[i].degree15  ||           
                  myDegree == sAUD027Picklist[i].degree16  ||           
                  myDegree == sAUD027Picklist[i].degree17  ||           
                  myDegree == sAUD027Picklist[i].degree18  ||           
                  myDegree == sAUD027Picklist[i].degree19  ||              
                  myDegree == sAUD027Picklist[i].degree20)              
              {                                                                 
                 myMajor = StripAmpHTML(sAUD027Picklist[i].majorlit)         //1.03
                 oForm.MAJOR.options[PicklistLen] =                             
                   new Option(myMajor, sAUD027Picklist[i].major,0,0)             //1.03
                 PicklistLen++;                                                 
              }                                                                 
           }                                                                    
        }                                                                       
    }                                                                           
    // 1.1h End                                                                 
                                                                                
    ///////////////////////////////////////////////////   //1.03 added          
    // StripHTML                                                                
    // Strip out "&#39;" and replace with "'"                                   
    // Strip out "&amp;" and replace with "&"                                   
    // These are added in by web11 (web04) when these characters exist in UCX   
    ///////////////////////////////////////////////////                         
    function StripAmpHTML (sString)                                             
    {                                                                           
     do
       {
       // Strip out "&#39;" and replace with "'"                                
       strPos = sString.indexOf("&#39;")                                        
       if (strPos != -1)                                                        
          sString = sString.replace("&#39;", "'");                              
       } // do-while
     while (strPos != -1)
                                                                                
     do
       {
       // Strip out "&amp;" and replace with "&"                                
       strPos = sString.indexOf("&amp;")                                        
       if (strPos != -1)                                                        
          sString = sString.replace("&amp;", "&");                              
       } // do-while
     while (strPos != -1)
                                                                                
     return sString;                                                          
    }                                                                           
                                                                                
    ///////////////////////////////////////////////////
    // OVERBLOCKLIMIT
    // Enforce restrictions on the number of each type of block you can have.
    ///////////////////////////////////////////////////
    function OverBlockLimit(oBlocks)
    {
		var MAX_WHATIF_BLOCKS = 75;
		//alert("oBlocks.length = " + oBlocks.length);
		if (oBlocks.length >= MAX_WHATIF_BLOCKS)
		{
			return true;
		}
		else
		{
			return false;
		}
    } // overblocklimit

    ///////////////////////////////////////////////////                         
    function AddBlock(sBlockValue, sBlockText, sBlockType)                      
    {                                                                           
        var oBlocks = document.frmWhatIfBody.BLOCKS;                            
        // If we still have our dummy value then get rid of it                  
        // We needed it to make the select box the correct width in Netscape 4.7
                                                                                
        if (oBlocks.length > 0 && oBlocks[0].value == "DUMMY")                  
        {                                                                       
            //oBlocks[0].selected = true;                                       
            //RemoveBlock();                                                    
            --oBlocks.length;                                                   
        }                                                                       
        // Code: 1.1d Fixed the add item more then once bug                     
        var tmpAlreadyAdded = false;                                            
        for (i=0; i < oBlocks.length; i++)                                      
        {                                                                       
			sThisBlockText = oBlocks.options[i].text;
			sThisBlockType = Trim(sThisBlockText.substring(0, sThisBlockText.indexOf(":")));
			//alert('sThisBlockType = [' + sThisBlockType + ']');
			sThisBlockText = Trim(sThisBlockText.substring(sThisBlockText.indexOf(":") + 2, sThisBlockText.length));
			//alert('sThisBlockText = [' + sThisBlockText + ']');
            if (oBlocks.options[i].value == sBlockValue &&     
			    sThisBlockType           == Trim(sBlockType) &&
				sThisBlockText           == Trim(sBlockText))
            {                                                                   
                tmpAlreadyAdded = true;                                         
            }                                                                   
        }                                                                       
        if (!tmpAlreadyAdded)                                                   
        {                                                                       
            oBlocks.length++;                                                   
            oBlocks.options[oBlocks.length - 1].value = sBlockValue;            
            oBlocks.options[oBlocks.length - 1].text = sBlockType + ": " +      
                                                       sBlockText;              
<$ILUCXMASK SRC=UCXCFG020 KEY=PLANNER BYTES=82:1 value=Y>                          
<$ILCGIMASK ACTION="SEPUSAGE">                                                  
            ToggleWhatIfCheckbox();                                             
</$ILCGIMASK>                                                                   
</$ILUCXMASK>                                                                   
        }  // End 1.1d Fix                                                      
    }                                                                           
    ///////////////////////////////////////////////////                         
    function SetBlock(thisSelect)                                               
    {                                                                           
        var oBlocks = document.frmWhatIfBody.BLOCKS;
        // Code: 1.1c Added if statment to correct add blank or label bug       
        if (thisSelect.options[thisSelect.selectedIndex].value != "" &&         
            thisSelect.options[thisSelect.selectedIndex].value != "            ")   
        {                                                                       
            //alert (thisSelect.name + " was clicked");                         
            sBlockValue = thisSelect.options[thisSelect.selectedIndex].value;   
            sBlockText = thisSelect.options[thisSelect.selectedIndex].text;     
            sBlockType = thisSelect.name;                                                   
           // Code: 1.1e Fix Name Padding Error                                
            for (i = sBlockType.length; i < 7; i++)                             
            {                                                                   
               sBlockType = sBlockType + " ";                                  
            } // End 1.1e Fix                                                   
            if (! OverBlockLimit (oBlocks))                                  
            {                                                                   
                AddBlock(sBlockValue, sBlockText, sBlockType);                  
            }                                                                   
        } // End 1.1c Fix                                                       
        // Code: 1.1c Added to correct select add bug                           
        if (thisSelect.ChangeStatus)                                            
        {                                                                       
            thisSelect.ChangeStatus = false;                                    
        } // End 1.1c Fix                                                       
        thisSelect.selectedIndex = 0;                                           
        thisSelect.focus();                                                     
    }                                                                           
    ///////////////////////////////////////////////////                         
    function RemoveBlock()                                                      
    {                                                                           
        var oBlocks = document.frmWhatIfBody.BLOCKS;                            
        if (oBlocks.length == 0)                                                
        {                                                                       
            alert("The block list is empty so nothing can be removed.");        
        }                                                                       
        else if (oBlocks.selectedIndex < 0)                                     
        {                                                                       
            alert ("You must first select a block before clicking Remove");     
        }                                                                       
        else // Something is selected                                           
        {                                                                       
            var tmpSelectItemCount = 0;                                         
            for (var i = 0; i < oBlocks.options.length; i++)                    
            {                                                                   
                if (!oBlocks.options[i].selected)                               
                {                                                               
                    oBlocks.options[tmpSelectItemCount].value =                 
                                                oBlocks.options[i].value        
                    oBlocks.options[tmpSelectItemCount].text =                  
                                                oBlocks.options[i].text         
                    ++tmpSelectItemCount;                                       
                }                                                               
            }                                                                   
            oBlocks.length = tmpSelectItemCount;                                
            oBlocks.selectedIndex = oBlocks.length -1;                          
<$ILUCXMASK SRC=UCXCFG020 KEY=PLANNER BYTES=82:1 value=Y>                          
<$ILCGIMASK ACTION="SEPUSAGE">                                                  
            ToggleWhatIfCheckbox();                                             
</$ILCGIMASK>                                                                   
</$ILUCXMASK>                                                                   
                                                                                
        }                                                                       
    } // removeblock                                                            
</script>                                                                       
