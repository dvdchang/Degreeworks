    //////////////////////////////////////////////////////////////////
    // Since AUD027 only allows up to 5 degrees we allow an entry ending
    // in _2 to specify a continuation so that up to 10 degrees can be used
    // for any particular major.
    // Example:
    // Major of ABC can have another ABC_2 entry in AUD027 with extra degrees.
    //////////////////////////////////////////////////////////////////                                                                                
    function AddPicklistItem (sMajor, sMajorLit, sCollege, 
                              sDegree1, sDegree2, sDegree3, sDegree4, sDegree5,
							                sSchool, sSingleDegree)                         
    {                                                                           
      sMajor = Trim (sMajor);
      // See if this is a continuation of a former item (probably the previous one)
      if ((sMajor.indexOf ("_2") > 0  || 
           sMajor.indexOf ("_3") > 0  || 
           sMajor.indexOf ("_4") > 0 ) &&
          sAUD027Picklist.length >= 1)
      {
		// Find the "base" major in the existing picklist.
		for (iPicklistIndex = 0; iPicklistIndex < sAUD027Picklist.length ; iPicklistIndex++ )
		{
			sThisMajor = sMajor.substring (0, sMajor.length - 2); // MATH_2 -> MATH
			if (sThisMajor == sAUD027Picklist[iPicklistIndex].major) // if this major is already in the list
			{
			/*alert("sThisMajor = " + sThisMajor + "\n" + 
			      "sMajor = " + sMajor + "\n" + 
				  "sAUD027Picklist[iPicklistIndex].major = " + sAUD027Picklist[iPicklistIndex].major);*/
				// We have found the major that we want to give these degrees to.
				if (sMajor.indexOf ("_2") > 0 )
				{
				sAUD027Picklist[iPicklistIndex].degree6  = Trim(sDegree1);
				sAUD027Picklist[iPicklistIndex].degree7  = Trim(sDegree2);
				sAUD027Picklist[iPicklistIndex].degree8  = Trim(sDegree3);
				sAUD027Picklist[iPicklistIndex].degree9  = Trim(sDegree4);
				sAUD027Picklist[iPicklistIndex].degree10 = Trim(sDegree5);				
				}
				else if (sMajor.indexOf ("_3") > 0 )
        {
				sAUD027Picklist[iPicklistIndex].degree11  = Trim(sDegree1);
				sAUD027Picklist[iPicklistIndex].degree12  = Trim(sDegree2);
				sAUD027Picklist[iPicklistIndex].degree13  = Trim(sDegree3);
				sAUD027Picklist[iPicklistIndex].degree14  = Trim(sDegree4);
				sAUD027Picklist[iPicklistIndex].degree15  = Trim(sDegree5);				
				}
				else if (sMajor.indexOf ("_4") > 0 )
          {
				sAUD027Picklist[iPicklistIndex].degree16  = Trim(sDegree1);
				sAUD027Picklist[iPicklistIndex].degree17  = Trim(sDegree2);
				sAUD027Picklist[iPicklistIndex].degree18  = Trim(sDegree3);
				sAUD027Picklist[iPicklistIndex].degree19  = Trim(sDegree4);
				sAUD027Picklist[iPicklistIndex].degree20  = Trim(sDegree5);				
				}
			break;
			}
          }
        }
	  
      else // add this new item
        {
        sAUD027Picklist[sAUD027Picklist.length] =
           new PicklistItem(sMajor, sMajorLit, sCollege, 
                            sDegree1, sDegree2, sDegree3, sDegree4, sDegree5,
							sSchool, sSingleDegree);                         
        }
    }
    //////////////////////////////////////////////////////////////////                                                                                
    function PicklistItem(sMajor, sMajorLit, sCollege, sDegree1, sDegree2,      
                          sDegree3, sDegree4, sDegree5, sSchool, sSingleDegree)                         
    {                                                                           
        this.major = sMajor;                                                    
        this.majorlit = sMajorLit;                                              
        this.college = sCollege;                                                
        this.school = sSchool;                                                
		    this.singleDegree = sSingleDegree;
        this.degree1 = Trim(sDegree1);                                                
        this.degree2 = Trim(sDegree2);                                                
        this.degree3 = Trim(sDegree3);                                                
        this.degree4 = Trim(sDegree4);                                                
        this.degree5 = Trim(sDegree5);                                                
        this.degree6  = "";
        this.degree7  = "";
        this.degree8  = "";
        this.degree9  = "";
        this.degree10 = "";
        this.degree11  = "";
        this.degree12  = "";
        this.degree13  = "";
        this.degree14  = "";
        this.degree15 = "";
        this.degree16 = "";
        this.degree17  = "";
        this.degree18  = "";
        this.degree19  = "";
        this.degree20  = "";
    }                                                                           
                                                                                
    sAUD027Picklist = new Array();                                                 
    sAUD029Picklist = new Array();                                                 
                                                                                
                                                                                
