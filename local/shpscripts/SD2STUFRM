<script language="JavaScript">                                                  
function tmpCheckValue()                                                        
{                                                                               
    var tmpCheckThis;                                                           
    tmpCheckThis = prompt("Check This:","");                                    
    if (tmpCheckThis != "close")                                                
    {                                                                           
        alert(tmpCheckThis + ": " + eval(tmpCheckThis));                        
        tmpCheckValue();                                                        
    }                                                                           
}                                                                               
</script>                                                                       
<script language="JavaScript">                                                  
top.SearchSubmitForm = SubmitForm;                                              
objKeyEventHandler.fAddKeyPressAction("ctrl+shift+s",function(){tmpCheckValue();
                                                                                
});                                                                             
</script>                                                                       
