Student Selection                                                               
<$ILINCLUDE SCRIPT=SD2INCHD2>                                                   
<script language="JavaScript">                                                  
var global_FullName  = "";                                                      
var global_FirstName = "";                                                      
var global_LastName  = "";           

var bAdvisorSearch = false; // obsolete really

    // is_nav and is_ie are defined in BrowserSniffer.js in inc hd2 script           
    if (is_nav4)                                                                
    {                                                                           
        document.captureEvents(Event.KEYDOWN|Event.KEYUP);                      
    }                                                                           
    // All browsers                                                             
    document.onkeydown = keyDown;                                               
    // When the user presses the Enter/Return key we will submit the form       
    function keyDown(DnEvents)                                                  
    {                                                                           
        var iKey = (is_nav) ? DnEvents.which : window.event.keyCode;            
        if (iKey == 13)                                                         
        {                                                                       
            SubmitForm();                                                       
            return false;                                                       
        }                                                                       
    }                                                                           
	///////////////////////////////////////////////////////////////////////////
    function FormatNameSearch (LastName, FirstName)        /* 1.04 Begin */     
    {                                                                           
        var sFullName          = "";                                            
        var bLastNameEntered   = false;                                         
        var bFirstNameEntered  = false;                                         
        var bLastNameWildcard  = false;                                         
        var bFirstNameWildcard = false;                                         
        var sFirstName         = "";                                            
        var sLastName          = "";                                            
                                                                                
        if (Trim(LastName) != "")                                               
        {                                                                       
            bLastNameEntered = true;                                            
            sLastName = Trim(LastName.toUpperCase());                           
            if (sLastName.indexOf('@') > -1)                                    
                bLastNameWildcard = true;                                       
        }                                                                       
        if (Trim(FirstName) != "")                                              
        {                                                                       
            bFirstNameEntered = true;                                           
            sFirstName = Trim(FirstName.toUpperCase());                         
            if (sFirstName.indexOf('@') > -1)                                   
                bFirstNameWildcard = true;                                      
        }                                                                       
                                                                                
        /*alert("bLastNameEntered   = " + bLastNameEntered   + "\n" +           
                "bFirstNameEntered    = " + bFirstNameEntered   + "\n" +        
                "bLastNameWildcard    = " + bLastNameWildcard   + "\n" +        
                "bFirstNameWildcard   = " + bFirstNameWildcard  + "\n" +        
                "sFirstName           = " + sFirstName + "\n" +                 
                "sLastName            = " + sLastName);*/                       
                                                                                
        // Both First and Last name entered                                     
        if (bFirstNameEntered && bLastNameEntered)                              
        {                                                                       
            // no wildcards at all                                              
            if (!bLastNameWildcard && !bFirstNameWildcard)                      
            {                                                                   
                sFullName = sLastName + "," + sFirstName + "@";                 
            }                                                                   
            // First name wildcard, but last name no wildcard                   
            else if (!bLastNameWildcard && bFirstNameWildcard)                  
            {                                                                   
                s = sFirstName; //use this to shorten next line.                
                if (s.substring(s.length-1,s.length).indexOf('@') > -1)         
                {                                                               
                    sFullName = sLastName + "," + s; // user supplied own       
                                            // wildcard in last supplied byte   
                }                                                               
                else // user supplied own wildcard - but not in last byte.      
                {                                                               
                    // include one for middle name and for partial name search  
                    sFullName = sLastName + "," + sFirstName + '@';             
                }                                                               
            }                                                                   
            else if (bLastNameWildcard && !bFirstNameWildcard)                  
            {                                                                   
                // same as first one above                                      
                sFullName = sLastName + "," + sFirstName + "@";                 
            }                                                                   
            else // both have wildcards                                         
            {                                                                   
                s = sFirstName; //use this to shorten next line.                
                if (s.substring(s.length-1,s.length).indexOf('@') > -1)         
                    // user supplied own wildcard in last supplied byte         
                    sFirstName = s;                                             
                else // wildcard not in last byte                               
                    sFirstName = s + '@';                                       
                sFullName = sLastName + "," + sFirstName;                       
            }                                                                   
        }                                                                       
                                                                                
        // First name only                                                      
        else if (bFirstNameEntered && !bLastNameEntered)                        
        {                                                                       
            if (!bFirstNameWildcard)                                            
                sFullName = "@," + sFirstName + "@";                            
            else                                                                
            {                                                                   
                if (sFirstName.indexOf('@') == 0)                               
                    // user supplied own wildcard in 1st byte                   
                    sFullName = sFirstName;                                     
                sFullName = "@," + sFirstName; // user supplied own wildcard    
                                              // but not in 1st byte. include   
                                              // one for the last name.         
            }                                                                   
        }                                                                       
                                                                                
        // Last name only                                                       
        else if (!bFirstNameEntered && bLastNameEntered)                        
        {                                                                       
            if (!bLastNameWildcard)                                             
                sFullName = sLastName + "@";                                    
            else                                                                
            {                                                                   
              if (LastName.substring(LastName.length-2,                         
                                     LastName.length-1).indexOf('@') > -1)      
                    // user supplied own wildcard in last supplied byte         
                    sFullName = sLastName;                                      
                else                                                            
                    // user supplied own wildcard but not in last byte.         
                    sFullName = sLastName + '@'; // include one for first name  
            }                                                                   
        }                                                                       
        else if (!bFirstNameEntered && !bLastNameEntered)                       
        {// no name supplied -- this is OK.                                     
        }                                                                       
        global_FullName  = sFullName;                                           
        global_FirstName = sFirstName;                                          
        global_LastName  = sLastName;                                           
    }                                                                           
/* 1.04 End */                                                                  
                                                                                
//////////////////////////////////////////////////////////////////////////
function SubmitForm()                                                           
{ 
var frmThis = document.frmHoldFields;                                                                                                             
if (bAdvisorSearch)
  {
  frmThis.SEARCHTYPE.value="ADVISOR";
  sWaitPage = 'SD_AdviseeWait.html';                                  
  }
else // not an advisor search
  {
  frmThis.SEARCHTYPE.value="ANY";
  sWaitPage = 'SD_SearchWait.html';                                   

/* 1.04 Begin */                                                                
        var E4012 = "";                                                         
        FormatNameSearch(frmThis.LastName.value, frmThis.FirstName.value);      
        E4012 = global_FullName;                                                
        frmThis.E4012.value = E4012; // Set the hidden form element             
		// Upshift the stu-id in case the user entered a lowercase value
        frmThis.E0001.value = frmThis.E0001.value.toUpperCase();                               

<$ILUCXMASK SRC=UCXCFG020 KEY=BANNER BYTES=15:1 value=Y DEP=BANNERSEARCH>          
        // BANNER SEARCH IS ENABLED                                             
       frmThis.EBA02.value = frmThis.E0001.value;                               
       frmThis.EBA11.value = global_LastName;                                   
       frmThis.EBA12.value = global_FirstName;                                  
       frmThis.E4012.value = ""; //blank out RAD name...Banner search only.     
       frmThis.E0001.value = ""; //blank out RAD id  ...Banner search only.     
</$ILUCXMASK>                                                                   

<$ILMASK Not DEP=BANNERSEARCH>  // search in RAD                                
		var sBeginSet         = "ER5RS=^&"; // RS=Repeat Set
		var sDegreeElement    = "ER513="; // on rad-goalDATA-dtl
		var sSchoolElement    = "ER512="; // on rad-goalDATA-dtl
		var sGoalCodeElement  = "ER515="; // goal-code  on goalDATA-dtl
		var sGoalValueElement = "ER516="; // goal-value on goalDATA-dtl
		var sCustomCodeElement  = "ER322="; // custom-code  on custom-dtl
		var sCustomValueElement = "ER323="; // custom-value on custom-dtl
		bDegreeUsedInSearchCriteria = false;
		bSchoolUsedInSearchCriteria = false;
		oElements = frmThis.elements;
		oMyCriteria = frmThis.MYCRITERIA;
		oMyCriteria.value = "&";
		// See if Degree or School were selected 
		for (iIndex = 0; iIndex < oElements.length; iIndex++)
		{
			// The 1st item is "All XXXX Codes";
			// index 0 means nothing was chosen
			if (oElements[iIndex].type.substring(0,6) == "select" &&
				oElements[iIndex].selectedIndex > 0)
			{
				selectedIndex = oElements[iIndex].selectedIndex;
				if (oElements[iIndex].name == "ER503")
				{
				  bDegreeUsedInSearchCriteria = true;
				  sDegreeElement += Trim(oElements[iIndex][selectedIndex].value) + "&";
				}
				else if (oElements[iIndex].name == "ER502")
				{
				  bSchoolUsedInSearchCriteria = true;
				  sSchoolElement += Trim(oElements[iIndex][selectedIndex].value) + "&";
				}
			}
		} // for
		// See if there are any non-repeatable custom data items selected
		// These are not put into our CRITERIA list when select is picked
		for (iIndex = 0; iIndex < oElements.length; iIndex++)
		{
			if (oElements[iIndex].type.substring(0,6) == "select" &&
				oElements[iIndex].selectedIndex > 0)
			{
	          if (oElements[iIndex].id.indexOf("Custom") != -1) // (sWhichTable == "CustomDtl") 
				{
				  selectedIndex = oElements[iIndex].selectedIndex;
		          oMyCriteria.value += sBeginSet; // the next two items are a "set"
				  sSearchType  = Trim(oElements[iIndex].name);                 // eg: SPORT
			      sSearchValue = Trim(oElements[iIndex][selectedIndex].value); // eg: SOCCER
			      oMyCriteria.value += sCustomCodeElement  + sSearchType  + "&"; // eg: ER322=SPORT&
			      if (sSearchValue != "-ALL-")
			       oMyCriteria.value += sCustomValueElement + sSearchValue + "&"; // eg: ER323=SOCCER&
				}
			}
		} // for
		
		// CMU Localization === Advisor search
		if (frmThis.ADVISORS.value != "")
		{
			for (iIndex = 0; iIndex < oElements.length; iIndex++)
			{
				if (Trim(oElements[iIndex].name) == "ADVISORS")
				{
				  oMyCriteria.value += sBeginSet; // the next two items are a "set"
				  sSearchType  = Trim(oElements[iIndex].name);                 // eg: ADVISOR
				  sSearchValue = Trim(oElements[iIndex].value); 
				  oMyCriteria.value += "ER515=ADVISOR&"; // hard code goaldata code name
			          oMyCriteria.value += sGoalValueElement + sSearchValue + "&"; // eg: ER516=ID&
				}
			} // for
		} // if
		// CMU Localization === End of Advisor search
		
		// Loop through all of the items that were added to our CRITERIA list
		// by calling the AddSearchItem method when selected
        var oCriteria = document.frmHoldFields.CRITERIA;
        for (i=0; i < oCriteria.length; i++)
        {
			sThisSearchText = oCriteria.options[i].text; // eg: MAJOR: Intro to Accounting
			sThisSearchType = Trim(sThisSearchText.substring(0, sThisSearchText.indexOf(":"))); // eg: MAJOR

			sValue = Trim(oCriteria.options[i].value); // eg: ACCT$GoaldataDtl 
			sThisSearchTable = sValue.substring(sValue.indexOf("$") + 1, sValue.length); // eg: GoaldataDtl
			sThisSearchValue = Trim(sValue.substring(0, sValue.indexOf("$"))); // eg: ACCT

		    oMyCriteria.value += sBeginSet; // the next two items are a "set"
			if (sThisSearchTable == "CustomDtl") 
			  {
			  oMyCriteria.value += sCustomCodeElement  + sThisSearchType  + "&"; // eg: ER322=SPORT&
			  if (sThisSearchValue != "-ALL-")
			    oMyCriteria.value += sCustomValueElement + sThisSearchValue + "&"; // eg: ER323=SOCCER&
			  }
            else // default is the GoaldataDtl
			  {
		      if (bDegreeUsedInSearchCriteria) // enforce this degree on the goaldata-dtl
			    oMyCriteria.value += sDegreeElement;
		      if (bSchoolUsedInSearchCriteria) // enforce this school on the goaldata-dtl
			  	oMyCriteria.value += sSchoolElement;
			  oMyCriteria.value += sGoalCodeElement  + sThisSearchType  + "&"; // eg: ER515=MAJOR&
			  oMyCriteria.value += sGoalValueElement + '"' + sThisSearchValue + '"&'; // eg: ER516=ACCT&
			  }
        }

		//alert("oMyCriteria.value = " + oMyCriteria.value);
        // CUSTOM search item: optional picklist - may not be defined
		// This was created before we allowed numerous "CustomDtl" search items to be used
		if (typeof (frmThis.CUSTOM) != "undefined")
          {
		  oCustom = frmThis.CUSTOM;
          if (oCustom.selectedIndex > 0)
            // Add the custom-code to the front; ex: "HOMECAMPUS    DUBLIN"
            frmThis.E7100.value = frmThis.CUSTOMCODE.value + oCustom.options[oCustom.selectedIndex].value;
          else // not selected
            frmThis.E7100.value = "";
          }

</$ILMASK>

        // If seach on entire database is Not allowed                           
<$ILUCXMASK SRC=UCXCFG020 KEY=SEARCH BYTES=21:1 value=N>                           
        // Be sure at least one field is filled in                              
        var bSomethingEntered = false;                                          
        // E4012 is "Name" //                                                   
        if (Trim (frmThis.E0001.value) != ""                                    
            //|| Trim (frmThis.E4012.value) != ""                               
<$ILUCXMASK NOT SRC=UCXCFG020 KEY=SEARCH BYTES=23:1 value=N> <!-- SSN Used? -->    
            || Trim (frmThis.E0014.value) != ""                                 
</$ILUCXMASK>                                                                   
            || Trim (frmThis.LastName.value) != ""                              
            || Trim (frmThis.FirstName.value) != ""                            
			|| Trim (frmThis.ADVISORS.value) != "")  // CMU Localization === Add input box to verification routine  for Advisor search    			
        {                                                                       
            bSomethingEntered = true;                                           
        }                                                                       
        // If no id, name or ssn was entered then check to see if something was 
        // chosen from the picklists                                            
        if (! bSomethingEntered)                                                
        {                                                                       
            oElements = frmThis.elements;                                       
            for (iIndex = 0; iIndex < oElements.length; iIndex++)               
            {                                                                   
                // The 1st item is "All XXXX Codes";                            
                // index 0 means nothing was chosen                             
                if (oElements[iIndex].type.substring(0,6) == "select"           
                    && oElements[iIndex].selectedIndex > 0)                     
                {                                                               
                    bSomethingEntered = true;                                   
                    break;                                                      
                }                                                               
            } // for            
			// Check the myCriteria string also
            if (document.frmHoldFields.CRITERIA.length > 0)
              bSomethingEntered = true;
            if (! bSomethingEntered)                                            
            {                                                                   
                alert("You must first enter search criteria before clicking Search"); 
                return;                                                         
            } // not somethingentered                                           
        } // not somethingentered                                               
</$ILUCXMASK> <<<<<< Seach on entire database not allowed                       
}
               
parent.frBody.location.href = sWaitPage;                                
document.frmHoldFields.submit();                                        
} submitform
///////////////////////////////////////////////////////
// Only repeatable items such as major, minor and custom elements
// should be added to this search list. The user may want
// to search on MAJOR=CHEM and MAJOR=ENGL - for example.
// Data values which appear only once in the database
// such as degree, school, level should not be added.
///////////////////////////////////////////////////////
function AddSearchItem(thisSelect)
{
//alert ("AddSearchItem enter");
    var oCriteria = document.frmHoldFields.CRITERIA;
	var sWhichTable = "unknown";
    // Code: 1.1c Added if statment to correct add blank or label bug					 
    if (thisSelect.options[thisSelect.selectedIndex].value != "" &&
        thisSelect.selectedIndex != 0                            &&
        thisSelect.options[thisSelect.selectedIndex].value != "            ")
    {
	    // Use the Id field to tell us if this is for the custom-dtl
	    if (thisSelect.id.indexOf("Custom") != -1)
	      sWhichTable = "CustomDtl";

        //alert (thisSelect.name + " was clicked");
        sSearchValue = thisSelect.options[thisSelect.selectedIndex].value;
        sSearchText = thisSelect.options[thisSelect.selectedIndex].text;
        sSearchType = thisSelect.name;
        AddToSearchList (sSearchValue, sSearchText, sSearchType, sWhichTable);
    } 
    // correct select add bug
    //if (thisSelect.ChangeStatus)
    //{
    //    thisSelect.ChangeStatus = false;
    //} 
    thisSelect.selectedIndex = 0;
    thisSelect.focus();
}
///////////////////////////////////////////////////
function AddToSearchList (sSearchValue, sSearchText, sSearchType, sWhichTable)
{
//alert ("AddToSearchList enter");
    var oCriteria = document.frmHoldFields.CRITERIA;
	// See if this item is already in our list
    var tmpAlreadyAdded = false;
    for (i=0; i < oCriteria.length; i++)
    {
		sThisSearchText = oCriteria.options[i].text; 
		sThisSearchType = Trim(sThisSearchText.substring(0, sThisSearchText.indexOf(":")));
		sThisSearchText = Trim(sThisSearchText.substring(sThisSearchText.indexOf(":") + 2, sThisSearchText.length));
		sValue = Trim(oCriteria.options[i].value); // eg: ACCT$GoaldataDtl 
		sThisSearchValue = Trim(sValue.substring(0, sValue.indexOf("$"))); // eg: ACCT
        if (sThisSearchValue == Trim(sSearchValue) &&
		    sThisSearchType  == Trim(sSearchType) &&
			sThisSearchText  == Trim(sSearchText))
        {
		    // Search item already in our list
            tmpAlreadyAdded = true;
        }
    }
	// If not already in our list
    if (!tmpAlreadyAdded)
    {
        oCriteria.length++;
        oCriteria.options[oCriteria.length - 1].value = sSearchValue + "$" + sWhichTable; // eg: ACCT$GoaldataDtl
        oCriteria.options[oCriteria.length - 1].text  = sSearchType + ": " + // eg: MAJOR
                                                        sSearchText;  // eg: Intro to Accounting
    } 
}
///////////////////////////////////////////////////
function RemoveSearchItem()
{
    var oCriteria = document.frmHoldFields.CRITERIA;
    if (oCriteria.length == 0)
    {
        alert("The list is empty so nothing can be removed.");
    }
    else if (oCriteria.selectedIndex < 0)
    {
        alert ("You must first select a search item before clicking Remove");
    }
    else // Something is selected
    {
        var tmpSelectItemCount = 0;
        for (var i = 0; i < oCriteria.options.length; i++)
        {
            if (!oCriteria.options[i].selected)
            {
                oCriteria.options[tmpSelectItemCount].value =
                                            oCriteria.options[i].value
                oCriteria.options[tmpSelectItemCount].text =
                                            oCriteria.options[i].text
                ++tmpSelectItemCount;
            }
        }
        oCriteria.length = tmpSelectItemCount;
        oCriteria.selectedIndex = oCriteria.length -1;

    }
} // removesearchitem
///////////////////////////////////////////////////
function ClearCriteriaList()
{
    var oCriteria = document.frmHoldFields.CRITERIA;
    oCriteria.length = 0;
} // clearcriterialist
</script>                                                                       
